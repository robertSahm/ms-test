@media only screen and (max-width: 420px) { //380 and under

  .name h1 a {
    font-size: .75em !important;
    margin-left: -10%;
    font-weight: 400;
    font-family: 'klinicslabbold-webfont';
  }

  .blockWrapper {
    //margin: 0 auto !important;
    padding: 0 !important;
    max-width: 100%;

    .imgClick {
      width: 87%;
      margin: 0 auto;
    }

    img {
      width: 100% !important;
      height: auto;
      margin: 0 auto !important;
      padding: 5% 0 !important;
      
    }
  }

  ul.navUl {
    padding-right: 17px;
  }

  h4.desc {
    font-size: 1em !important;
  }


  ul.title-area {
    li.toggle-topbar {
      height: 35px;
      //top: 29px !important;
      a {
        //height: 50px;
        span {
          width: 31px !important;
          //height: 2px !important;
        }
      }
    }
  }

  .headerWrapper {
    max-height: 119px;
  }

  .descWrapper {
    position: relative;
    top: 5px !important;
  }

  .top-bar {
    min-height: 70px;
  }





} //CLOSE 380 and under

@media only screen and (min-width: 420px) { // Between 420 and 800


}//end

