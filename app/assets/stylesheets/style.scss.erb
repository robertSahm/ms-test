$body-bg: #EFECCA;
$body-font-color: #444;

//Home Colors
$orange		 :#DE5605;
$yellow		 :#F7A035;	
$blueLight :#B1DEB5;
$blueDark  :#65ABA6;
$white     :#EFECCA;

//Porta Colors
$bluePorta: #00BDFF;
$redPorta:  #BE4D4B;
$bgPorta:   #000000;

//Ketchum Colors
$yellowKetch: #CAC9AA;
$brownKetch: #684F40;
$blueKetch: #47BCBA;

