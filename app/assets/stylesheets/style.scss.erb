$body-bg: #EFECCA;
$body-font-color: #444;
$yellowKetchum: #DFDDB4;
$bluePorta: #00BDFF;
$redPorta: #BE4D4B;

$orange		 :#DE5605;
$yellow		 :#F7A035;	
$blueLight :#B1DEB5;
$blueDark  :#65ABA6;
$white     :#EFECCA;


//$blueDark: #5D718C;


// :before, :after { -webkit-transform: translateZ(0); }
