//   -webkit-transform: translate3d(0, 0, 0) !important;

body {
	background-color: $body-bg;
}

.headerWrapper {
	position: relative;
	top: 0;
	left: 0;
	min-height: 30px;
	margin-top: 3%;

	nav.top-bar {
	  margin-bottom: 0px !important;
	  position: relative;
	  display: block;
	  min-height: 30px;

		.titleRow {
			position: relative;
			top: 0;
			left: 0;

			ul.titleUl {
				padding-bottom: .08% !important;

				li.name {

					h1.name {
						@include fontMed;
						font-weight: 400;
						white-space: nowrap;
						overflow: show;
						background-color: $body-bg;
						color: lighten($body-bg, 50%);
						text-align: center;
						position: relative;
						top: 0;
						left: 0;
					
						//Header Title Kearning fixes
						.loosen1 {
							letter-spacing: .02em;
						}
						.tighten1 {
							letter-spacing: -.015em;
						}
						.tighten2 {
							letter-spacing: -.01em;
						}
						.loosen2 {
							letter-spacing: .012em;
						}
					}
				}
			}
		}
	}

	section.top-bar-section { // must be block level (height in this case)
		height: 1%;
		//border: 1px solid red;
		.navRow {
	
			div {
				ul.navUl {
					background-color: $body-bg;
  				width: 100%;
  				display: block;
					margin: 0% auto !important;
  				text-align: center;
				
					li.navLi {
						display: inline-block;
						margin: 0 !important;
						float: none;
						@include fontBook;

						:hover, :active, :target {
  					  color: $yellow;
  					  @include fontBookIt;
  					}
				
						a.navLink {
  					  width: 100%;
  					  height: 100%;
  					  margin-top: 30%; // nav push up
  					  margin-bottom: 10%; //nav push down
  					  line-height: .2em; // height of links
  					  // margin: 0 !important;
  					  // padding: 0 !important;
  					}
					}
				}
			}
		}
	}
}

.navSelected {
	@include fontBookIt;
	color: $yellow !important;
}

.descWrapper {
	position: relative;
	background-color: $body-bg;
	margin-top: 20px;
	max-height: 10px;

	.topLineRow {
		position: relative;
		top: 0;
		left: 0;

		.topLine {
			position: relative;
			top: 0;
			left: 0;
			height: 10%;
			border-bottom: 1px solid $yellow;
		}
	}

	.descRow {

		.descWrap {
			position: relative;
			top: 0px;
			left: 0;
			margin: 0 auto;
			top: -13px; // moves words up
			line-height: 0;
			height: .5em;
			//width: 40%;
			margin-top: 10px;

			h4.desc {
				color: $yellow;
				text-align: center;
				@include fontBookIt;
				padding: 0;
				background-color: $body-bg;
				line-height: .4em;
				font-size: 1.2em;
				width: 49%;
				min-width: 100px;
				height: auto;
				margin: 0 auto;
			}
		}
	}
}
