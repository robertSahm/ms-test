//Font Book 
@mixin fontBook {
	font-family: 'klinicslabbook-webfont';
}
@mixin fontBookIt {
	font-family: 'klinicslabbookit-webfont';
}

//Font Bold
@mixin fontBold {
	font-family: 'klinicslabbold-webfont';
}
@mixin fontBoldIt {
	font-family: 'klinicslabboldit-webfont';
}

//Font Medium
@mixin fontMed {
	font-family: 'klinicslabmedium-webfont';
}
@mixin fontMedIt {
	font-family: 'klinicslabmediumit-webfont';
}

//Font Light
@mixin fontLight {
	font-family: 'klinicslablight-webfont';
}
@mixin fontLightIt{
	font-family: 'klinicslablightIt-webfont';
}

