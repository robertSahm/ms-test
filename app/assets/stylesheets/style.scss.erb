//HOVER WORDS

.hoverText {
	display: none;
	font-size: .9em !important;
	@include fontBook;
	text-align: center;
	z-index: 100;

	p {
		margin: 5% 0 0 0;

		a {
			display: block;
			@include fontBold;
			font-weight: 400;
			position: relative;
			margin-top: 0;
			padding: 0 0 0 0;
			line-height: .8em;
		}
	}
}

.portaHoverP {
	color: $bluePorta; 
}

.portaHoverA a {
	color: $redPorta;
}

.ketchHoverText, .ketchHoverP {
	color: $brownKetch;
	@include fontBold;
}

p.carolHoverP {
	color: $blueCarol;

	a {
		color: $redCarol;
	}
}

p.c2cHoverP {
	@include fontBold;
	color: white;

	a {
		color: $blueC2C;
	}
}

.crawHoverText, .crawHoverP {
	color: $brownKetch;

	a {
		margin-top: -70px;
	}
}

.candlHoverP {
	color: #999;

	 a {
	 	color: black;
	 	margin-bottom: 50px;
	 }
}



