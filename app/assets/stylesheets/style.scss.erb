
body {
  width: 1125px;
}

/*Font Styling*/
h1.contentTitle {
  text-transform: uppercase;
  letter-spacing: .08em;
  font-weight: bold;
  font-family: "TradeGothic LT CondEighteen";
  font-size: 12px;
  margin: 0 0 5px 0;
}

h2.contentHeader {
  color: #4C89A6;
  font-weight: 100;
  font-family: "TradeGothic LT CondEighteen";
  font-size: 20px;
  margin: 0 0 10px 0;
}

h3, h4 {
  font-family: 'verdana';
  font-size: 11px;
  line-height: .6em;
  color: #999;
}

a, a:hover, a:active, a:focus {
  color: #999;
}

/* Top Nav Area Styling*/
ul {
  list-style: none;
  padding: 0;
  margin: 0;
}

.nav-wrap {
  width: 1025px;
  height: 159px;
  background-image: url("header_bg.png");
  padding-top: 49px;
  margin-bottom: 38px;
}

.nav-1 {
  //margin-bottom: 17px;
  margin-bottom: 6px;
  margin-left: 329px;
  height: 31px
}

.nav-2-ul {
  height: 31px
}

.nav2 {
  margin: 0;
  padding: 0;
}

ul.nav-1-ul li, 
ul.nav-2-ul li,
ul.nav-3-ul li {
  display: inline-block;
  margin: 0;
  padding: 0;
}

form.searchForm {
 font-size: 12px;
}

