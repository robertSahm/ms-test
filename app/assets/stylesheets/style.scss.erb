/* Left Sub-Nav */
.leftSubNav {
  float: left;
  display: block;
  width: 261px;
  height: 531px;
  padding-left: 44px;
}



.subNavHeader {
  padding-bottom: 10px;
}

.indented {
  margin-left: 36px;
}

.greenText {
  color: #A9BD4C;
}

/* Main Content  */
.mainContent {
  float: left;
  width: 470px;
  height: 531px;
  padding-left: 20px;
  border-right: 1px solid #ccc;
  border-left: 1px solid #ccc;
}

.contentP {
  padding-right: 20px;
  font-weight: bold;
  line-height: 1.6em;
  font-family: "Verdana";
  font-size: 10px;
}

/* Pop ups */
.popUp1, .popUp2 {
  display: none;
  position: relative;
  width: 400px;
  height: 300px;
  margin: 0 auto;
  z-index: 100;
  border-radius: 25px;
  background-image: url("header_bg.png");
  //background-color: #3F7C9B;
}

.popUp2 {
  height: 90px;
}

.popUp2 p {
  color: white;
  position: absolute;
  width: 400px;
  height: 100px;
  text-align: center;
  font-size: 12px;
  padding-top: 30px;
  font-family: verdana;
}

a.pop2 {
  margin: 0;
  padding: 0;
}

