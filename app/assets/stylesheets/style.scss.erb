.top-bar.expanded {
  background: $body-bg; 
}

.left li {
	padding-top: 10px;
	white-space: nowrap;
	padding-bottom: 0;
	background-color: $body-bg;

	a {
		background-color: $body-bg !important;
		@include fontBook;
	}
}

.navWrap {
	position: relative;
	top: 30px;
	left: 0;
	max-width: 680px;
	margin: 0 auto;
	margin-bottom: 30px;
	background-color: $body-bg;

	.navBlock {
		position: relative;
		top: 3px; // lines up nav test w name
		left: 0px;
		background-color: $body-bg;

		a {
			background-color: $body-bg !important;
		}
	}
}

h1.name {
	@include fontMed;
	font-weight: 400;
	font-size: 67px !important;
	white-space: nowrap;
	overflow: show;
	background-color: $body-bg;
}

.expanded {
	//top: -10px;
	margin: 0;
	padding: 0;
	background-color: $white;
	
	ul.left {
		//display: inline;
		height: 80px;
		text-align: left;
		padding: 0 0 0 0;
		margin: -10px 0 0 0;
		background-color: $body-bg !important;

		li {
			top: -20px;
			float: right;
			padding: 0;
			margin: 0;

			a {
  			display: table-cell; 
  			vertical-align: middle; 
  			text-align: center; 
  			padding: 15px !important;
			}
		}
	}
}

.titleRow {
	margin-bottom: 40px;
}

.greyRow {
	position: relative;
	top: 40px;
	left: 0;

	.greyLine {
		position: relative;
		top: 0px;
		left: 0;
		border-bottom: 1px solid $yellow;
		z-index: -100;
		//margin-bottom: 90px;
	}
}

.descWrap {
	position: relative;
	margin: 0 auto;
	top: -3px;
	line-height: 0;
	height: .5em;
	//width: 40%;
	margin-top: 40px;

	h4.desc {
		color: $yellow;
		text-align: center;
		position: relative;
		@include fontBookIt;
		padding: 0;
		background-color: $body-bg;
		line-height: .4em;
		font-size: 1.6em;
		width: 49%;
		min-width: 200px;
		height: auto;
		margin: 0 auto;
	}
}

