.footerBlocker {
	position: relative;
	bottom: 0;
	left: 0;
	width: 100%;
	height: 50px;
}

#sticky_footer {
 	position: fixed;
 	bottom: 0px;
 	left: 0px;
 	width: 100%;
 	background-color: $darkBlue;
 	height: 2.5em;
 	z-index: 500;
 	opacity: .8;
 	box-shadow: 0px 1px 6px 1px rgba(0, 0, 0, 0.45);
 	padding-right: 1%;
 	font-size: .8em;
 	-webkit-transition: all .2s linear;
 	transition: all .2s linear;

 	&:hover {
 		height: 4.0em;
 		opacity: 1;
 	}

  	p.footer1 {
  		padding-top: .1em;
  		@include fontMed;
  		color: $yellow;
  		text-align: right;
  	}
}

