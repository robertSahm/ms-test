<div class="row blocksWrapper bbq-default">

<!-- Ketch Block Wrapper -->
  <div id="ketchBlockWrapper" class="large-6 columns blockWrapper ketchReveal">
    <div id="ketchClick" class="imgClick small-7 small-centered columns">
      <div class="hoverFade">
        <%= image_tag "ketchBlockCol.png", :class => "ketchBlockCol top blockImg" %>
        <%= image_tag "ketchBlockBW.png", :class => "ketchBlockBW bottom blockImg" %>
      </div>
    </div>
    <div class="ketchHoverText hoverText large-12">
      <p class="ketchHoverP">
        HTML, CSS, JS, design
      </p>
      <p class="ketchHoverP"><a class="ketchReveal">see more</a></p>
    </div>
  </div>


<!-- Porta Block Wrapper -->
  <div id="portaBlockWrapper" class="large-6 columns blockWrapper portaReveal">
