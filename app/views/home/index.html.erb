				<%= image_tag "crawBlockBW.png", :class => "crawBlockBW bottom blockImg" %>
			</div>
		</div>
	</div>

	<div id="candlBlockWrapper" class="large-4 small-12 columns blockWrapper">
		<div id="candlClick" class="imgClick">
			<div class="hoverFade">
				<%= image_tag "candlBlockCol.png", :class => "candlBlockCol centered top blockImg" %>
